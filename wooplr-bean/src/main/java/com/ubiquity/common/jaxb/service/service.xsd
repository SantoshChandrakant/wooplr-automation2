<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="serviceDetails">
    <xs:sequence>
      <xs:element name="category" type="xs:long" minOccurs="0"/>
      <xs:element name="description" type="xs:string" minOccurs="0"/>
      <xs:element name="id" type="xs:long" minOccurs="0"/>
      <xs:element name="name" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
   <xs:complexType name="serviceAttributes">
    <xs:sequence>
      <xs:element name="id" type="xs:long"/>
       <xs:element name="serviceId" type="xs:long"/>
      <xs:element name="attributes" type="xs:long" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
   <xs:element name="serviceForm" type="serviceDetails"/>
</xs:schema>

